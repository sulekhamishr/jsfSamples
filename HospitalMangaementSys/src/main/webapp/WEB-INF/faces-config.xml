<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd" 
              version="2.0" metadata-complete="false">


    <managed-bean>
        <managed-bean-name>doctorDao</managed-bean-name>
        <managed-bean-class>com.java.hms.jsf.dao.HmsDaoImp</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>doctor</managed-bean-name>
        <managed-bean-class>com.java.hms.jsf.model.Doctor</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    
        <managed-bean>
        <managed-bean-name>patientDao</managed-bean-name>
        <managed-bean-class>com.java.hms.jsf.dao.HmsDaoImp</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>patient</managed-bean-name>
        <managed-bean-class>com.java.hms.jsf.model.Patient</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>medication</managed-bean-name>
        <managed-bean-class>com.java.hms.jsf.model.MedicationHistory</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-name>medicationDao</managed-bean-name>
                <managed-bean-class>com.java.hms.jsf.dao.HmsDaoImp</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    
    

    <managed-bean>
        <managed-bean-name>hospitalController</managed-bean-name>
        <managed-bean-class>com.java.hms.jsf.controller.HospitalController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>    
   
    
        <managed-property>
            <property-name>doctorDao</property-name>
            <property-class>com.java.hms.jsf.dao.HmsDaoImp</property-class>
            <value>#{doctorDao}</value>
    </managed-property>
    <managed-property>
            <property-name>doctor</property-name>
            <property-class>com.java.hms.jsf.model.Doctor</property-class>
            <value>#{doctor}</value>
    </managed-property>
    
    
        <managed-property>
            <property-name>patientDao</property-name>
            <property-class>com.java.hms.jsf.dao.HmsDaoImp</property-class>
            <value>#{patientDao}</value>
    </managed-property>
    
    <managed-property>
            <property-name>patient</property-name>
            <property-class>com.java.hms.jsf.model.Patient</property-class>
            <value>#{patient}</value>
                </managed-property>
            
            
                    <managed-property>
            <property-name>medicationDao</property-name>
            <property-class>com.java.hms.jsf.dao.HmsDaoImp</property-class>
            <value>#{medicationDao}</value>
    </managed-property>
    
    
    <managed-property>
            <property-name>medication</property-name>
            <property-class>com.java.hms.jsf.model.MedicationHistory</property-class>
            <value>#{medication}</value>
    </managed-property>
    

    </managed-bean>

</faces-config>



